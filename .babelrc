{
  "presets": [
    [
      "@babel/preset-env",
      {
        "corejs": 3,
        "useBuiltIns": "usage",
        "modules": false,
        "loose": true,
        "targets": ["last 1 version", "> 1%", "not dead", "not IE 11", "not ie_mob 11"],
        "debug": true
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": ["@babel/plugin-transform-typescript"]
}
